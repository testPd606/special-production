<div class="swiper2 position-relative">
    <div class="d-lg-flex align-items-lg-end justify-content-lg-between mb-7 mb-lg-9">
        <div class="decoration-bar position-relative mb-7 mb-lg-0">

            <span class="badge rounded-pill  bg-primary-300 mb-2">
                <%- capsule %>
            </span>
            <h4 class="fs-lg-1  text-black mb-2">
                <%- title %>
            </h4>
            <h6 class="fs-lg-5  text-gray-400 fw-normal">
                <%- subtitle %>
            </h6>
        </div>
        <div class="d-flex">
            <!-- prev -->
            <div
                class="swiper-button-prev-custom mt-0 position-static w-auto h-auto text-gray-400 rounded-4 bg-gray-100 p-3 me-6">
                <span class="material-icons fs-lg-2">
                    arrow_back
                </span>

            </div>
            <!-- next -->
            <div
                class="swiper-button-next-custom mt-0 position-static w-auto h-auto text-gray-400 rounded-4 bg-gray-100 p-3">
                <span class="material-icons fs-lg-2">
                    arrow_forward
                </span>
            </div>
        </div>
    </div>
    <div class="swiper-wrapper d-flex align-items-center">
        <% slides.forEach(function(slide) { %>
            <div class="swiper-slide overflow-hidden">
                <%- include('card', slide) %>
            </div>
            <% }) %>


    </div>
</div>